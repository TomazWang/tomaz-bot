    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "firebase-admin": "^5.8.2",
    "morgan": "~1.9.0",
    "request": "^2.83.0",
    "serve-favicon": "~2.4.5",
    "uuid": "^3.2.1",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "node-fetch": "^2.0.0"
