{
  "name": "tomaz-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --require babel-register -R spec && codecov",
    "lint": "./node_modules/.bin/eslint ./app.js ./routes/ ./lib/",
    "start": "node -r babel-register ./bin/www",
